<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!-- toolbaractionbar颜色 -->
        <item name="colorPrimary">#11cd6e</item>
        <!-- 状态栏颜色 -->
        <item name="colorPrimaryDark">#11cd6e</item>
        <!-- 窗口的背景颜色 -->
        <item name="android:windowBackground">@android:color/white</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--custom theme -->
    <style name="AppTheme1" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
